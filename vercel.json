{
  "rewrites": [
    {
      "source": "/oauth/kakao/callback",
      "destination": "/"
    },
    {
      "source": "/oauth/google/callback",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/oauth2/authorization/kakao",
      "destination": "https://quizy.n-e.kr/oauth2/authorization/kakao",
      "permanent": false
    },
    {
      "source": "/oauth2/authorization/google",
      "destination": "https://quizy.n-e.kr/oauth2/authorization/google",
      "permanent": false
    }
  ]
}
